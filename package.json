{
    "name": "my-ts-project",
    "version": "1.0.0",
    "description": "A simple TypeScript starter project for learning and experimentation",
    "main": "dist/index.js",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/devcronberg/my-ts-project"
    },
    "bugs": {
        "url": "https://github.com/devcronberg/my-ts-project/issues"
    },
    "homepage": "https://github.com/devcronberg/my-ts-project#readme",
    "scripts": {
        "build": "tsc",
        "watch": "tsc --watch",
        "start": "node dist/index.js",
        "dev": "tsc && node dist/index.js",
        "test": "tsc && node --test dist/**/*.test.js",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "format": "prettier --write \"src/**/*.ts\""
    },
