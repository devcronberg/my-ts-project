        "eslint": "^9.39.1",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-sonarjs": "^3.0.5",
        "prettier": "^3.6.2",
        "typescript": "^5.9.3"
    },
    "dependencies": {
        "chalk": "^5.6.2"
    }
